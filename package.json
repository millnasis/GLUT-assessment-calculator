{
  "name": "GLUT_assessment-calculator",
  "productName": "GLUT_assessment-calculator",
  "version": "1.0.0",
  "description": "桂工综测计算器",
  "main": ".webpack/main",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "keywords": [],
  "author": "millnasis",
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "name": "GLUT_assessment-calculator",
        "icon": "./src/source/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-zip"
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/main/index.html",
                  "js": "./src/main/index.jsx",
                  "name": "main_window",
                  "preload": {
                    "js": "./src/preload.js"
                  }
                },
                {
                  "html": "./src/setting/index.html",
                  "js": "./src/setting/index.jsx",
                  "name": "setting",
                  "preload": {
                    "js": "./src/preload.js"
                  }
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "browser": {
    "path": false
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@babel/preset-react": "^7.14.5",
    "@electron-forge/cli": "^6.0.0-beta.64",
    "@electron-forge/maker-deb": "^6.0.0-beta.64",
    "@electron-forge/maker-rpm": "^6.0.0-beta.64",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.64",
    "@electron-forge/maker-zip": "^6.0.0-beta.64",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.64",
    "@vercel/webpack-asset-relocator-loader": "^1.7.0",
    "antd": "^4.19.1",
    "babel-loader": "^8.2.2",
    "core-js": "^3.15.2",
    "css-loader": "^6.2.0",
    "echarts": "^5.3.2",
    "electron": "^18.2.0",
    "electron-packager": "^15.5.1",
    "exceljs": "^4.3.0",
    "html-loader": "^2.1.2",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.1.0",
    "node-loader": "^2.0.0",
    "node-sass": "^6.0.1",
    "postcss": "^8.3.6",
    "postcss-loader": "^6.1.1",
    "postcss-preset-env": "^6.7.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-transition-group": "^4.4.2",
    "redux": "^4.1.2",
    "sass": "^1.35.2",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.2.1",
    "ts-loader": "^9.2.4",
    "typescript": "^4.3.5",
    "webpack": "^5.46.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "exceljs": "^4.3.0",
    "path-browserify": "^1.0.1"
  }
}
